using System;

namespace Exact Sum of Real Numbers
{
    class Program
    {
        static void Main(string[] args)
        {
            int n = int.Parse(Console.ReadLine());
            decimal sum = 0;
            ExactSum(n, sum);
        }

        static void ExactSum(int n, decimal sum)
        {
            for (int i = 0; i < n; i++)

            sum += decimal.Parse(Console.ReadLine());

            Console.WriteLine(sum);
        }
    }
}
