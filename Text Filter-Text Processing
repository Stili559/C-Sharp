using System;
using System.Linq;
using System.Text;

namespace Text Filter
{
    class Program
    {
        static void Main(string[] args)
        {
            string[] remove = Console.ReadLine().Split(", ").ToArray();
            StringBuilder Remove = new StringBuilder(Console.ReadLine());
            string command = "";

            foreach (var item in remove)
            {
                for(int i = 0; i < item.Length; i++)
                {
                    command += "*";
                }
                while (Remove.ToString().Contains(item))
                {
                    Remove.Replace(item, command);
                }
                command = "";
            }
            Console.WriteLine(Remove);
        }
    }
}
